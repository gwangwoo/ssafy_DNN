   "source": [
    "import numpy as np\n",
    "\n",
    "# softmax func\n",
    "def softmax(a):\n",
    "    c = np.max(a)\n",
    "    exp_a = np.exp(a-c)\n",
    "    sum_exp_a = np.sum(exp_a)\n",
    "    y = exp_a / sum_exp_a\n",
    "    return y\n",
    "\n",
    "\n",
    "a = np.array([3.0,4.0,2.1,5.3,6.3])\n",
    "y = softmax(a)\n",
    "print(y)"
   ]
  },
